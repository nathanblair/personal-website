{
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable"],
    "target": "esnext",
    "alwaysStrict": true,
    "checkJs": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
